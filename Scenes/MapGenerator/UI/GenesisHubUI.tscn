[sub_resource type="Shader" id="PlanetAuraShader"]
code = "shader_type canvas_item;

void fragment()
{
	vec2 p = UV * 2.0 - vec2(1.0);
	float r = length(p);
	float outer = 1.0 - smoothstep(0.72, 1.0, r);
	float inner = smoothstep(0.12, 0.54, r);
	float aura = clamp(outer * inner, 0.0, 1.0);
	COLOR = vec4(COLOR.rgb, COLOR.a * aura);
}"

